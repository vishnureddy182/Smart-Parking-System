<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Adminlog</servlet-name>
        <servlet-class>VehicleParking.Logs.Adminlog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>VehicleParking.Reg.UserRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>VehicleParking.Logs.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>slot_booking</servlet-name>
        <servlet-class>VehicleParking.QRCODE.slot_booking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TCLogs</servlet-name>
        <servlet-class>VehicleParking.Logs.TCLogs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParkingDetails</servlet-name>
        <servlet-class>VehicleParking.QRCODE.ParkingDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Adminlog</servlet-name>
        <url-pattern>/Adminlog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>slot_booking</servlet-name>
        <url-pattern>/slot_booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TCLogs</servlet-name>
        <url-pattern>/TCLogs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParkingDetails</servlet-name>
        <url-pattern>/ParkingDetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
